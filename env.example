# WeQuo Phase 1 Environment Variables
# Copy this file to .env and fill in your API keys

# FRED API (Required for economic data)
FRED_API_KEY=your_fred_api_key_here

# Alpha Vantage API (Optional - for commodities data, uses mock data if not provided)
ALPHA_VANTAGE_API_KEY=your_alpha_vantage_key_here

# New connectors (optional - add only if you have the keys)
ACLED_API_KEY=your_acled_api_key
ACLED_EMAIL=your_registered_email@example.com
NOAA_API_KEY=your_noaa_api_key
UNCOMTRADE_API_KEY=your_uncomtrade_subscription_key
MARINETRAFFIC_API_KEY=your_marinetraffic_api_key

# Web App Configuration
SECRET_KEY=your_secret_key_here
HOST=0.0.0.0
PORT=5000
DEBUG=false

# Stripe Payment Configuration
STRIPE_PUBLISHABLE_KEY=pk_test_your_stripe_publishable_key_here
STRIPE_SECRET_KEY=sk_test_your_stripe_secret_key_here
STRIPE_WEBHOOK_SECRET=whsec_your_stripe_webhook_secret_here